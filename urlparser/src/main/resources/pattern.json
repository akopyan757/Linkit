[
  {
    "pattern": "^.*/[a-z]{2}.wikipedia.org/.*",
    "is_base": true,
    "title_element": null,
    "description_element": null,
    "image_url_element": "head.link.apple-touch-icon.."
  },
  {
    "pattern": "^.*/[a-z]{2}.wikipedia.org/wiki/.*",
    "is_base": false,
    "title_element": null,
    "description_element": null,
    "image_url_element": "head.meta.property.og:image"
  },
  {
    "pattern": "^.*/medium.com/.*",
    "is_base": true,
    "title_element": "head.title",
    "description_element": "head.meta.description",
    "image_url_element": "head.link.apple-touch-icon..120x120"
  },
  {
    "pattern": "^.*/medium.com/[@a-z0-9]{1,}/[-a-z0-9]{1,}",
    "is_base": false,
    "title_element": "head.meta.property.og:title",
    "description_element": "head.meta.property.og:description",
    "image_url_element": "head.meta.property.og:image"
  }
]